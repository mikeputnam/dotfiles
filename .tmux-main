source-file ~/.tmux.conf.theme

rename-session -t 0 main
rename-window -t 0 vagrant
send-keys -t 0 'cd ~/src/ssm-alpha && vagrant up default && vagrant ssh default' C-m
new-window -n repo -c ~/src/ssm-alpha
new-window -n bash
new-window -n servers       "ssh www2"
split-window -h -p 25 -t 0  "ssh cron0"
split-window -h -p 25 -t 0  "ssh staging"
select-pane -t 0
select-layout even-vertical
select-window -t 0
